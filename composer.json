    "name": "qrstuff/scout-mongodb",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-mongodb": "*",
        "mongodb/laravel-mongodb": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "illuminate/database": ">=8.0",
        "illuminate/support": ">=8.0",
        "laravel/scout": ">=8.0"
    },
    "type": "library"
}
