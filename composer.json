    "description": "MongoDB (not MongoDB Atlas) engine for the Laravel Scout search.",
    "extra": {
        "laravel": {
            "providers": [
                "QRStuff\\Scout\\MongoDB\\ScoutMongoDBServiceProvider"
            ]
        }
